// Pattern 9: Multiple components (5+)
component Alpha {
	type AlphaData {
		id: string
	}
}

component Beta {
	type BetaData {
		name: string
	}
}

component Gamma {
	type GammaData {
		count: int
	}
}

component Delta {
	type DeltaData {
		value: float
	}
}

component Epsilon {
	type EpsilonData {
		flag: bool
	}
}

component Zeta {
	type ZetaData {
		description: string
	}
}
